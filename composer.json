{
  "name": "haryadi/haryadi",
  "description": "Haryadi Framework - a MVC micro-framework",
  "type": "library",
  "license": "MIT",
  "autoload": {
    "psr-4": {
      "Haryadi\\": "src/Haryadi/",
      "App\\": "app/"
    }
  },
  "bin": [
    "bin/haryadi"
  ],
  "require": {
    "php": ">=8.1",
    "vlucas/phpdotenv": "^5.5",
    "symfony/var-dumper": "^6.3",
    "haryadi/haryadi": "dev-main"
  },
  "repositories": [
    {
      "type": "vcs",
      "url": "https://github.com/Captzuzo/haryadi"
    }
  ],
  "minimum-stability": "dev",
  "prefer-stable": true
}
